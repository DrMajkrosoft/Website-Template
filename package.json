{
  "name": "fe-template",
  "productName": "Website Template",
  "version": "4.4.1",
  "description": "Boilerplate for the Single Page Application. Everything is already set up and ready to use.",
  "main": "index.html",
  "scripts": {
    "start": "ws",
    "browserify": "browserify ./src/js/app.js -o ./src/js/main.js --debug",
    "build": "npm run browserify && gulp && node ./src/js/sitemap",
    "eslint": "eslint --init",
    "test": "ava --verbose ./test/main.test.js",
    "test:wdio": "wdio ./test/wdio.test.js",
    "allure": "allure generate ./allure-results && allure open",
    "babel": "babel ./src/js/app.js -d babel",
    "nyc": "nyc ava --verbose ./src/js",
    "coverage": "nyc report --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VladimirDev93/Website-Template.git"
  },
  "author": "Vladimir Jovanovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VladimirDev93/Website-Template/issues"
  },
  "engines": {
    "node": "latest",
    "npm": "latest"
  },
  "homepage": "https://github.com/VladimirDev93/Website-Template#readme",
  "devDependencies": {
    "allure-commandline": "^2.8.1",
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "browserify": "^16.2.3",
    "chromedriver": "^2.44.0",
    "eslint": "^5.9.0",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^5.0.3",
    "gulp-minify-html": "^1.0.6",
    "gulp-uglify": "^3.0.1",
    "local-web-server": "^2.6.0",
    "nyc": "^13.1.0",
    "sitemap": "^2.1.0",
    "uglify-es": "^3.3.9",
    "wdio-allure-reporter": "^0.8.3",
    "wdio-chromedriver-service": "^0.1.3",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-selenium-standalone-service": "0.0.11",
    "webdriverio": "^4.14.0"
  }
}
