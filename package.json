{
  "name": "fe-template",
  "productName": "Website Template",
  "version": "4.4.8",
  "description": "Boilerplate for the Single Page Application. Everything is already set up and ready to use.",
  "main": "index.html",
  "scripts": {
    "start": "ws",
    "browserify": "browserify ./src/js/main.js -o ./src/js/transformed.js --debug",
    "build": "npm run browserify && gulp && node ./src/js/sitemap",
    "eslint": "eslint --init",
    "test": "ava --verbose ./test/main.test.js",
    "test:wdio": "wdio ./test/wdio.test.js",
    "allure": "allure generate ./allure-results && allure open",
    "babel": "babel ./src/js/app.js -d babel",
    "nyc": "nyc ava --verbose ./src/js",
    "coverage": "nyc report --reporter=html"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VladimirDev93/Website-Template.git"
  },
  "author": "Vladimir Jovanovic",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/VladimirDev93/Website-Template/issues"
  },
  "engines": {
    "node": "10",
    "npm": "latest"
  },
  "homepage": "https://github.com/VladimirDev93/Website-Template#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "allure-commandline": "^2.9.0",
    "autoprefixer": "^9.4.7",
    "ava": "^1.2.1",
    "babel-cli": "^6.26.0",
    "browserify": "^16.2.3",
    "chromedriver": "^2.45.0",
    "eslint": "^5.12.1",
    "gulp": "^4.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^3.0.1",
    "gulp-imagemin": "^5.0.3",
    "gulp-minify-html": "^1.0.6",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-uglify": "^3.0.1",
    "local-web-server": "^2.6.1",
    "navigo": "^7.1.2",
    "nyc": "^13.1.0",
    "postcss-nested": "^4.1.1",
    "postcss-preset-env": "^6.5.0",
    "postcss-safe-parser": "^4.0.1",
    "precss": "^4.0.0",
    "sitemap": "^2.1.0",
    "uglify-es": "^3.3.9",
    "wdio-allure-reporter": "^0.8.3",
    "wdio-chromedriver-service": "^0.1.5",
    "wdio-mocha-framework": "^0.6.4",
    "wdio-selenium-standalone-service": "0.0.12",
    "webdriverio": "^5.4.15"
  },
  "postcss": {
    "plugins": {
      "postcss-nested": {},
      "postcss-preset-env": {},
      "autoprefixer": {
        "browsers": [
          "last 2 versions"
        ]
      }
    },
    "parser": "postcss-safe-parser"
  },
  "dependencies": {}
}
